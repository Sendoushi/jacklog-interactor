{
  "name": "jacklog-interactor",
  "version": "0.0.1",
  "description": "Interactor with Jacklog system",
  "main": "lib/index.js",
  "engines": {
    "node": "10.15.3",
    "npm": "6.4.1"
  },
  "scripts": {
    "clean:build": "rm -rf lib",
    "predev": "npm run clean:build",
    "dev": "cross-env NODE_ENV=development webpack --watch --info-verbosity verbose",
    "prebuild": "npm run clean:build",
    "build": "cross-env NODE_ENV=development webpack",
    "prebuild:prod": "npm run clean:build",
    "build:prod": "cross-env NODE_ENV=production webpack; mv module/src/index.d.ts module/index.d.ts; rm -rf module/src module/webpack.config.d.ts; ",
    "build:watch": "npm run dev",
    "lint:all": "pretty-quick --bail; echo \"Eslint started...\"; eslint \"src/**/**.+(ts)\" --fix -c .eslintrc",
    "lint": "pretty-quick --bail --staged; node ./scripts/lint.js",
    "test": "jest",
    "onpush": "npm run lint; npm run test; npm run build:prod; git add ."
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run onpush"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@types/jest": "^24.9.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.12.11",
    "@types/webpack": "^4.41.0",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "epic-spinners": "^1.1.0",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.1.0",
    "eslint-plugin-jsdoc": "^18.1.4",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "fork-ts-checker-webpack-plugin": "^3.1.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "pretty-quick": "^2.0.1",
    "stats-webpack-plugin": "^0.7.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.1.10",
    "webpack-extension-reloader": "^1.1.4"
  }
}
